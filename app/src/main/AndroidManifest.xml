<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vahan.dasamatyan">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Gnahatakanner" />
        <activity android:name=".GnahatelActivity" />
        <activity android:name=".KcelAshakertinDasaran" />
        <activity android:name=".KcelDasaxosinActivity" />
        <activity android:name=".ArarkaAvelacnelActivity" />
        <activity android:name=".DasaranAvelacnelActivity" />
        <activity android:name=".AshakertActivity" />
        <activity android:name=".Dasaran" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".usanox.Usanox_gaxtnabar"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".usanox.Usanox_anhatakan" />
        <activity
            android:name=".dasaxos.Dasaxos_gaxtnabar"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".dasaxos.Dasaxos_anhatakan"
            android:label="@string/title_activity_dasaxos_anhatakan"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Grancvel" />
        <activity android:name=".dasaxos.DasaxosArarkanerActivity" />
        <activity android:name=".Admin" />
        <activity android:name=".Admin_gaxtnabar" />
    </application>

</manifest>